<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="stu.back.org.mapper.CourseMapper">
    <select id="selectAll" resultType="stu.back.org.domain.Course">
        select * from course
    </select>
    <select id="selectOne" resultType="stu.back.org.domain.Course">
        select * from course where id=#{id}
    </select>
</mapper>